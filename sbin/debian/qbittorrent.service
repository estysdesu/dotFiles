# linuxbabe.com/ubuntu/install-qbittorrent-ubuntu-18-04-desktop-server

Description=qBittorrent

Wants=network.target
After=syslog.target network-online.target

[Service]
Type=simple
ExecStart=/usr/bin/qbittorrent
Restart=on-failure
RestartSec=10
KillMode=process

[Install]
WantedBy=multi-user.target
